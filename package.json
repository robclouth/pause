{
  "name": "pause",
  "author": "Rob Clouth",
  "repository": "https://github.com/robclouth/pause",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "build": {
    "appId": "com.robclouth.pause",
    "productName": "Pause",
    "publish": [
      {
        "provider": "github",
        "owner": "robclouth",
        "repo": "pause"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "electron": "yarn expo-electron start",
    "pack": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "dist": "yarn electron-webpack && yarn electron-builder"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "active-win": "^6.1.0",
    "expo": "~37.0.3",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-screens": "~2.2.0",
    "react-native-web": "~0.11.7",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/electron-adapter": "^0.0.0-alpha.52",
    "@expo/webpack-config": "^0.12.4-alpha.0",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "babel-preset-expo": "~8.1.0",
    "electron": "^6.0.12",
    "electron-builder": "^22.6.0",
    "electron-config": "^2.0.0",
    "electron-updater": "^4.3.1",
    "typescript": "~3.8.3"
  },
  "private": true
}
